#!/bin/sh
#
# $Id$
#
# copy in arch specific packages
# (c) Erich Focht, NEC HPCE

oscar_rpm_pool=/tftpboot/rpm

# Remove any "pvm-*" RPMs in the /tftpboot/rpm dir e.g., RH9's pvm-3.4.4-12 

for file  in `ls -1 $oscar_rpm_pool | egrep -e "^pvm-"` ; do
    echo "  Remove pre-existing PVM file: $oscar_rpm_pool/$file"
    rm -f "$oscar_rpm_pool/$file"
done


# Do actual work...

$OSCAR_HOME/scripts/generic-setup 
